
<% observations.each do |observation| %>
  <td> 
    <table class="table">
        <tr><td class="text-left"><%= observation.id %></td></tr>
        <tr><td class="text-left"><%= observation.config_day %></td></tr>
        <tr><td class="text-left"><%= observation.m_bp %>・<%= observation.l_bp %>・<%= observation.d_bp %></td></tr>
        <tr><td class="text-left"><%= observation.m_kt %>・<%= observation.l_kt %>・<%= observation.d_kt %></td></tr>
        <tr><td class="text-left"><%= observation.m_p %>・<%= observation.l_p %>・<%= observation.d_p %></td></tr>
        <tr><td class="text-left"><%= observation.m_r %>・<%= observation.l_r %>・<%= observation.d_r %></td></tr>
        <tr><td class="text-left"><%= observation.m_o2root %>・<%= observation.l_o2root %>・<%= observation.d_o2root %></td></tr>
        <tr><td class="text-left"><%= observation.m_o2rate %>・<%= observation.l_o2rate %>・<%= observation.d_o2rate %></td></tr>
        <tr><td class="text-left"><%= observation.m_meal %>・<%= observation.l_meal %>・<%= observation.d_meal %></td></tr>
        <tr><td class="text-left"><%= observation.m_snack %>・<%= observation.l_snack %>・<%= observation.d_snack %></td></tr>
        <tr><td class="text-left"><%= observation.m_pain %>・<%= observation.l_pain %>・<%= observation.d_pain %></td></tr>
        <tr><td class="text-left"><%= observation.m_numbness %>・<%= observation.l_numbness %>・<%= observation.d_numbness %></td></tr>
        <tr><td class="text-left"><%= observation.m_drowsiness %>・<%= observation.l_drowsiness %>・<%= observation.d_drowsiness %></td></tr>
        <tr><td class="text-left"><%= observation.m_spo2 %>・<%= observation.l_spo2 %>・<%= observation.d_spo2 %></td></tr>
        <tr><td class="text-left"><%= observation.m_bs %>・<%= observation.l_bs %>・<%= observation.d_bs %></td></tr>
        <tr><td class="text-left"><%= observation.m_insulin %>・<%= observation.l_insulin %>・<%= observation.d_insulin %></td></tr>
      
        <%= hidden_field_tag :follow_id, user.id %>
        <tr><td class="text-left">
        <%= link_to '編集', edit_observation_path(observation), class: 'btn btn-info btn-xs' %>
        <%= link_to "削除", observation, method: :delete, data: { confirm: "You sure?" }, class: 'btn btn-danger btn-xs' %>
        </td></tr>
    </table>
  </td>
<% end %>


